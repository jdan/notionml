      "archived": false,
      "type": "paragraph",
      "paragraph": {
        "text": [
          {
            "type": "text",
            "text": {
              "content": "Make it as pixel-perfect as I reasonably can",
              "link": null
            },
            "annotations": {
              "bold": true,
              "italic": false,
              "strikethrough": false,
              "underline": false,
              "code": false,
              "color": "default"
            },
            "plain_text": "Make it as pixel-perfect as I reasonably can",
            "href": null
          },
          {
            "type": "text",
            "text": {
              "content": " as a creative exercise. I'm going for nostalgia points and I have to really nail it.",
              "link": null
            },
            "annotations": {
              "bold": false,
              "italic": false,
              "strikethrough": false,
              "underline": false,
              "code": false,
